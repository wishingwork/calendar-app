{
  "cli": {
    "version": ">= 16.14.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./weather-mission-google-service.json",
        "track": "internal",
        "releaseStatus": "draft"
      }      
    }
  }
}
